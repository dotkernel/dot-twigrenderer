{
  "name": "dotkernel/dot-twigrenderer",
  "type": "library",
  "description": "DotKernel component providing twig extensions and customization",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-expressive-twigrenderer": "^1.1",
    "zendframework/zend-view": "^2.8",
    "zendframework/zend-servicemanager": "^3.1",
    "ocramius/proxy-manager": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "dotkernel/dot-authorization": "~0.1",
    "dotkernel/dot-authentication": "~0.1",
    "dotkernel/dot-navigation": "~0.1",
    "dotkernel/dot-flashmessenger": "~0.1",

    "zendframework/zend-form": "^2.9"

  },
  "autoload": {
    "psr-4": {
      "Dot\\Twig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Twig\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  }
}
